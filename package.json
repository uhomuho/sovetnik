    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "force-ssl-heroku": "^1.0.2",
    "konva": "^7.1.3",
    "moment": "^2.28.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "serve-static": "^1.14.1",
